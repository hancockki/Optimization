0af457212702"],

Cell[BoxData[
 RowBox[{
  RowBox[{"-", 
   FractionBox[
    RowBox[{"4", " ", 
     SuperscriptBox["x", "2"]}], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "+", 
       SuperscriptBox["x", "2"]}], ")"}], "2"]]}], "+", 
  FractionBox["2", 
   RowBox[{"1", "+", 
    SuperscriptBox["x", "2"]}]]}]], "Output",
