Cell["Computing f\[CloseCurlyQuote]\[CloseCurlyQuote](x) for the first \
example above", "Subsection",
 CellChangeTimes->{{3.525187145748518*^9, 3.525187163916335*^9}, {
  3.5255302925105896`*^9, 3.52553029481939*^9}, {3.5255303964845896`*^9, 
  3.52553040807539*^9}, {3.52553044347179*^9, 
  3.5255304445013895`*^9}},ExpressionUUID->"fcd54719-202e-40d8-8c40-\
46a6a969148b"],

Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{"Log", "[", 
   RowBox[{
    RowBox[{"x", "^", "2"}], "+", "1"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.525187179398883*^9, 
  3.525187182640207*^9}},ExpressionUUID->"806740a5-c72c-46f6-bf40-\
4ebd41ed4899"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"df", "[", "x_", "]"}], "=", 
   RowBox[{"D", "[", 
    RowBox[{
     RowBox[{"f", "[", "x", "]"}], ",", "x"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"d2f", "[", "x_", "]"}], "=", 
  RowBox[{"D", "[", 
   RowBox[{
    RowBox[{"df", "[", "x", "]"}], ",", "x"}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.5251871848364267`*^9, 3.5251872173286753`*^9}, {
